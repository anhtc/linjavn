@using LinJas.Common
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@section Metas{
    <title>Giới thiệu về thiết kế web chuẩn SEO</title>


}
@section css{
    @Styles.Render("~/bundles/desktop/css-gioithieu")
}
@section scripts{

    <script type="text/javascript">
        setTimeout(function () {
            var e = document.createElement('script');
            e.src = '@Scripts.Url("~/bundles/desktop-gioithieu")';
                e.type = 'text/javascript';
                document.getElementsByTagName("head")[0].appendChild(e);
        }, 10);

    </script>
}
<!--#breadcrumb-->
